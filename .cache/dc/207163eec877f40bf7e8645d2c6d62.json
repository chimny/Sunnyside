;\r\n        transform: translate(-50%, -50%);\r\n\r\n        h1 {\r\n            color: v.$primaryWhite;\r\n            font-family: \"Fraunces\", serif;\r\n            font-size: 36px;\r\n            left: 50%;\r\n            letter-spacing: 0.5rem;\r\n            position: absolute;\r\n            text-align: center;\r\n            text-transform: uppercase;\r\n            top: 30%;\r\n            transform: translate(-50%, -50%);\r\n        }\r\n\r\n        img.arrow {\r\n            height: 72px;\r\n            margin-top: 7rem;\r\n        }\r\n    }\r\n\r\n    nav {\r\n        height: 80px;\r\n        margin-right: 0;\r\n        position: relative;\r\n        top: v.$positionValue;\r\n        width: 100%;\r\n\r\n        .burger {\r\n            position: absolute;\r\n            right: v.$positionValue;\r\n            z-index: 10;\r\n        }\r\n\r\n        .logo {\r\n            left: v.$positionValue;\r\n            position: absolute;\r\n        }\r\n    }\r\n}\r\n\r\nul {\r\n    background-color: v.$primaryWhite;\r\n    display: none;\r\n    font-family: \"Fraunces\", serif;\r\n    height: 14rem;\r\n    left: 50%;\r\n    margin: 0 auto;\r\n    position: absolute;\r\n    top: 12rem;\r\n    transform: translate(-50%, -50%);\r\n    width: 80vw;\r\n    z-index: 2;\r\n\r\n    img {\r\n        display: none;\r\n    }\r\n\r\n    li {\r\n        list-style: none;\r\n        margin: 0 auto;\r\n        text-align: center;\r\n        width: 50%;\r\n\r\n        a {\r\n            color: v.$neutralDarkGrayishblue;\r\n            display: block;\r\n            padding: 1rem;\r\n            text-decoration: none;\r\n            width: 100%;\r\n        }\r\n    }\r\n\r\n    li:last-child {\r\n        a {\r\n            background: v.$primaryYellow;\r\n            border-radius: v.$borderRadius;\r\n            color: v.$neutralDarkDesaturatedblue;\r\n            text-transform: uppercase;\r\n        }\r\n    }\r\n}\r\n\r\n@media (min-width: 769px) {\r\n    .wrapper {\r\n        max-width: v.$desktop;\r\n    }\r\n\r\n    header {\r\n        background-image: url(\"/images/desktop/image-header.jpg\");\r\n        max-width: v.$desktop;\r\n        min-height: 85vh;\r\n\r\n        .cta {\r\n            @include absolutePositioner(30%);\r\n\r\n            h1 {\r\n                display: block;\r\n                width: 50vw;\r\n            }\r\n\r\n            img.arrow {\r\n                @include absolutePositioner(50%);\r\n                height: 120px;\r\n                margin-top: 120px;\r\n            }\r\n        }\r\n    }\r\n\r\n    img.burger {\r\n        display: none;\r\n    }\r\n\r\n    ul {\r\n        background-color: transparent;\r\n        display: block;\r\n        height: 14rem;\r\n        margin: 0 auto;\r\n        padding-right: 64px;\r\n        position: absolute;\r\n        right: 0;\r\n        text-align: right;\r\n        top: 0;\r\n        transform: translateX(-50%);\r\n        width: 100%;\r\n        z-index: 2;\r\n\r\n        .container {\r\n            padding: 1rem 0;\r\n        }\r\n\r\n        img {\r\n            display: none;\r\n        }\r\n\r\n        li {\r\n            display: inline;\r\n            list-style: none;\r\n            margin: 0 1rem 0 auto;\r\n            text-align: center;\r\n            width: auto;\r\n\r\n            a {\r\n                background: v.$primarySoftRed;\r\n                border-radius: v.$borderRadius;\r\n                color: v.$primaryWhite;\r\n                display: inline;\r\n                padding: 1rem;\r\n                text-decoration: none;\r\n                width: auto;\r\n            }\r\n        }\r\n\r\n        li:last-child {\r\n            a {\r\n                background: v.$primaryWhite;\r\n                color: v.$neutralDarkDesaturatedblue;\r\n                text-transform: uppercase;\r\n            }\r\n        }\r\n    }\r\n}\r\n","scss\\variables.scss":"// layout\r\n$mobile: 768px;\r\n$mobilebreakpoint: 376px;\r\n$desktop: 1440px;\r\n$fontSizeMain: 16px;\r\n$positionValue: 32px;\r\n$borderRadius: 50px;\r\n\r\n// colors primary\r\n$primarySoftRed: hsl(7, 99%, 70%);\r\n$primaryYellow: hsl(51, 100%, 49%);\r\n$primaryDarkDesaturated: hsl(167, 40%, 24%);\r\n$primaryDarkblue: hsl(198, 62%, 26%);\r\n$primaryDarkModerateCyan: hsl(168, 34%, 41%);\r\n$primaryWhite: hsl(0, 0%, 100%);\r\n\r\n// colors neutral\r\n$neutralDarkDesaturatedblue: hsl(212, 27%, 19%);\r\n$neutralVeryDarkGrayishblue: hsl(213, 9%, 39%);\r\n$neutralDarkGrayishblue: hsl(232, 10%, 55%);\r\n$neutralGrayishblue: hsl(210, 4%, 67%);\r\n\r\n// font-family: 'Barlow', sans-serif;\r\n// font-family: 'Fraunces', serif;\r\n"},"lineCount":null}},"error":null,"hash":"3eae81ba4bae831f8ff94d33c39f442e","cacheData":{"env":{}}}